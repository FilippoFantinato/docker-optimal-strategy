MODULE controller(u0,u1,u2,u3,u4,u5)
VAR
	c0 : boolean;
	c1 : boolean;
	c2 : boolean;
	c3 : boolean;
	c4 : boolean;
	c5 : boolean;
ASSIGN
	c0 := u0;
	c1 := u1;
	c2 := u2;
	c3 := u3;
	c4 := u4;
	c5 := u5;
LTLSPEC F[0,6](c0 <-> u0) & X(F[0,6](c1 <-> u1)) & X X(F[0,6](c2 <-> u2)) & X X X(F[0,6](c3 <-> u3)) & X X X X(F[0,6](c4 <-> u4)) & X X X X X(F[0,6](c5 <-> u5))
VAR       time: 0..42;
FROZENVAR maxtime: 0..42;
ASSIGN
  init(time) := 0;
  next(time) := case time < 42 : time + 1; TRUE: time; esac;
PARSYNTH r := { maxtime | VALID  F[0,6]((c0 <-> u0) & time <= maxtime) & X(F[0,6]((c1 <-> u1) & time <= maxtime)) & X X(F[0,6]((c2 <-> u2) & time <= maxtime)) & X X X(F[0,6]((c3 <-> u3) & time <= maxtime)) & X X X X(F[0,6]((c4 <-> u4) & time <= maxtime)) & X X X X X(F[0,6]((c5 <-> u5) & time <= maxtime))};
