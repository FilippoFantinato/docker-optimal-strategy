MODULE controller(p)
--latches
VAR
EBR_0_PAST_3 : boolean;
EBR_0_ERROR_0 : boolean;
EBR_0_COUNTER_1 : boolean;
EBR_0_COUNTER_0 : boolean;
EBR_0_SPECF_2 : boolean;
ASSIGN
init(EBR_0_PAST_3) := FALSE;
next(EBR_0_PAST_3) := !a24;
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a54;
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a68;
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a72;
init(EBR_0_SPECF_2) := FALSE;
next(EBR_0_SPECF_2) := a8;
DEFINE
--ands
a10 := !a2 & a8;
a14 := EBR_0_PAST_3;
a16 := FALSE;
a18 := !a14 & !a16;
a20 := a10;
a22 := !a10 & !a18;
a24 := !a20 & !a22;
a28 := !EBR_0_ERROR_0 & EBR_0_PAST_3;
a30 := !a2 & a8;
a32 := a30 & !a4;
a34 := !EBR_0_ERROR_0 & a32;
a36 := !EBR_0_ERROR_0 & !a4;
a38 := FALSE;
a40 := !a36;
a42 := !a38 & !a40;
a44 := FALSE;
a46 := !a34 & !a42;
a48 := !a44 & !a46;
a50 := FALSE;
a52 := !a28 & !a48;
a54 := !a50 & !a52;
a60 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a62 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a64 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a66 := !a62 & !a64;
a68 := !a60 & !a66;
a70 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a72 := !a70 & EBR_0_COUNTER_0;
a76 := p & !EBR_0_SPECF_2;
a8 := !a76;
a78 := EBR_0_ERROR_0;
a2 := FALSE;
a4 := TRUE;
--outputs
formula := !a78;
q := a2;
s := a4;

LTLSPEC  (p) U (q U s)

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  ((p) U (q U (s & time<=maxtime)))};
