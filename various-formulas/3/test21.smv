MODULE controller(u)
--latches
VAR
EBR_0_ERROR_0 : boolean;
EBR_0_COUNTER_1 : boolean;
EBR_0_COUNTER_0 : boolean;
EBR_0_COUNTER_2 : boolean;
EBR_0_PAST_1 : boolean;
ASSIGN
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a56;
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a68;
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a74;
init(EBR_0_COUNTER_2) := FALSE;
next(EBR_0_COUNTER_2) := !a88;
init(EBR_0_PAST_1) := FALSE;
next(EBR_0_PAST_1) := !a110;
DEFINE
--ands
a14 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a16 := !EBR_0_COUNTER_2 & a14;
a22 := !EBR_0_ERROR_0 & EBR_0_PAST_1;
a24 := a2 & a4;
a26 := !u & !a24;
a28 := !EBR_0_ERROR_0 & a26;
a30 := a2 & a4;
a32 := !EBR_0_ERROR_0 & !a30;
a34 := FALSE;
a36 := !a32;
a38 := !a34 & !a36;
a40 := FALSE;
a42 := !a28 & !a38;
a44 := !a40 & !a42;
a46 := FALSE;
a48 := !a22 & !a44;
a50 := !a46 & !a48;
a52 := FALSE;
a54 := !a16 & !a50;
a56 := !a52 & !a54;
a58 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a60 := a58 & EBR_0_COUNTER_2;
a62 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a64 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a66 := !a62 & !a64;
a68 := !a60 & !a66;
a70 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a72 := a70 & EBR_0_COUNTER_2;
a74 := !a72 & EBR_0_COUNTER_0;
a76 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a78 := a76 & EBR_0_COUNTER_2;
a80 := EBR_0_COUNTER_1 & EBR_0_COUNTER_0;
a82 := a80 & EBR_0_COUNTER_2;
a84 := !EBR_0_COUNTER_2 & !a80;
a86 := !a82 & !a84;
a88 := !a78 & !a86;
a90 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a92 := !EBR_0_COUNTER_2 & a90;
a94 := EBR_0_PAST_1;
a96 := FALSE;
a98 := !a94 & !a96;
a100 := !u;
a102 := u & !a98;
a104 := !a100 & !a102;
a106 := FALSE;
a108 := !a92 & !a104;
a110 := !a106 & !a108;
a112 := EBR_0_ERROR_0;
a2 := TRUE;
a4 := TRUE;
--outputs
formula := !a112;
c1 := a2;
c2 := a4;

LTLSPEC  X (u U (c1 & c2))

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  X (u U (c1 & c2 & time<=maxtime))};
