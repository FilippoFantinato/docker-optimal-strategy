MODULE controller()
--latches
VAR
EBR_0_ERROR_0 : boolean;
EBR_0_COUNTER_1 : boolean;
EBR_0_COUNTER_0 : boolean;
ASSIGN
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a12;
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a26;
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a30;
DEFINE
--ands
a6 := !EBR_0_ERROR_0 & !a2;
a8 := FALSE;
a10 := !a6;
a12 := !a8 & !a10;
a18 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a20 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a22 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a24 := !a20 & !a22;
a26 := !a18 & !a24;
a28 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a30 := !a28 & EBR_0_COUNTER_0;
a32 := EBR_0_ERROR_0;
a2 := TRUE;
--outputs
formula := !a32;
c := a2;

LTLSPEC  F(c)

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  F (c & time<=maxtime)};
