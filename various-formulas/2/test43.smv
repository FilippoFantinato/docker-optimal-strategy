MODULE controller(p)
--latches
VAR
EBR_0_COUNTER_1 : boolean;
EBR_0_COUNTER_0 : boolean;
EBR_0_SPECF_3 : boolean;
EBR_0_COUNTER_2 : boolean;
EBR_0_SPECF_2 : boolean;
EBR_0_PAST_1 : boolean;
EBR_0_ERROR_0 : boolean;
ASSIGN
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a22;
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a28;
init(EBR_0_SPECF_3) := FALSE;
next(EBR_0_SPECF_3) := !a2;
init(EBR_0_COUNTER_2) := FALSE;
next(EBR_0_COUNTER_2) := !a44;
init(EBR_0_SPECF_2) := FALSE;
next(EBR_0_SPECF_2) := EBR_0_SPECF_3;
init(EBR_0_PAST_1) := FALSE;
next(EBR_0_PAST_1) := !a68;
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a104;
DEFINE
--ands
a10 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a14 := a10 & EBR_0_COUNTER_2;
a16 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a18 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a20 := !a16 & !a18;
a22 := !a14 & !a20;
a24 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a26 := a24 & EBR_0_COUNTER_2;
a28 := !a26 & EBR_0_COUNTER_0;
a32 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a34 := a32 & EBR_0_COUNTER_2;
a36 := EBR_0_COUNTER_1 & EBR_0_COUNTER_0;
a38 := a36 & EBR_0_COUNTER_2;
a40 := !EBR_0_COUNTER_2 & !a36;
a42 := !a38 & !a40;
a44 := !a34 & !a42;
a48 := !EBR_0_COUNTER_2 & !EBR_0_COUNTER_1;
a52 := EBR_0_PAST_1;
a54 := FALSE;
a56 := !a52 & !a54;
a58 := !p;
a60 := p & !a56;
a62 := !a58 & !a60;
a64 := FALSE;
a66 := !a48 & !a62;
a68 := !a64 & !a66;
a70 := !EBR_0_COUNTER_2 & !EBR_0_COUNTER_1;
a74 := !EBR_0_ERROR_0 & EBR_0_PAST_1;
a76 := !p & EBR_0_SPECF_2;
a78 := !EBR_0_ERROR_0 & a76;
a80 := !EBR_0_ERROR_0 & EBR_0_SPECF_2;
a82 := FALSE;
a84 := !a80;
a86 := !a82 & !a84;
a88 := FALSE;
a90 := !a78 & !a86;
a92 := !a88 & !a90;
a94 := FALSE;
a96 := !a74 & !a92;
a98 := !a94 & !a96;
a100 := FALSE;
a102 := !a70 & !a98;
a104 := !a100 & !a102;
a106 := EBR_0_ERROR_0;
a2 := TRUE;
--outputs
formula := !a106;
q := a2;

LTLSPEC  (X X p) U (q)

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  ((X X p) U (q & time<=maxtime))};
