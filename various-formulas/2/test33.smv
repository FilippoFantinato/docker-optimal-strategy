MODULE controller(u)
--latches
VAR
EBR_0_COUNTER_1 : boolean;
EBR_0_ERROR_1 : boolean;
EBR_0_COUNTER_0 : boolean;
EBR_0_COUNTER_2 : boolean;
EBR_0_ERROR_0 : boolean;
ASSIGN
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a22;
init(EBR_0_ERROR_1) := FALSE;
next(EBR_0_ERROR_1) := !a32;
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a38;
init(EBR_0_COUNTER_2) := FALSE;
next(EBR_0_COUNTER_2) := !a52;
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a76;
DEFINE
--ands
a10 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a14 := a10 & EBR_0_COUNTER_2;
a16 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a18 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a20 := !a16 & !a18;
a22 := !a14 & !a20;
a26 := !EBR_0_ERROR_1 & !u;
a28 := FALSE;
a30 := !a26;
a32 := !a28 & !a30;
a34 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a36 := a34 & EBR_0_COUNTER_2;
a38 := !a36 & EBR_0_COUNTER_0;
a40 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a42 := a40 & EBR_0_COUNTER_2;
a44 := EBR_0_COUNTER_1 & EBR_0_COUNTER_0;
a46 := a44 & EBR_0_COUNTER_2;
a48 := !EBR_0_COUNTER_2 & !a44;
a50 := !a46 & !a48;
a52 := !a42 & !a50;
a56 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a58 := !EBR_0_COUNTER_2 & a56;
a60 := !EBR_0_COUNTER_2 & !EBR_0_COUNTER_1;
a62 := !a58 & a60;
a64 := a62 & !a2;
a66 := a64;
a68 := FALSE;
a70 := !a66 & !a68;
a72 := EBR_0_ERROR_0;
a74 := !EBR_0_ERROR_0 & !a70;
a76 := !a72 & !a74;
a78 := !EBR_0_ERROR_0 & !EBR_0_ERROR_1;
a80 := !a78;
a2 := FALSE;
--outputs
formula := !a80;
c := a2;

LTLSPEC  X(c) -> F(u)

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  (X(c) -> F(u & time<=maxtime))};
