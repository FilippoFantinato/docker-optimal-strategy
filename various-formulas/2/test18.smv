MODULE controller(u)
--latches
VAR
EBR_0_PAST_1 : boolean;
EBR_0_ERROR_0 : boolean;
EBR_0_COUNTER_1 : boolean;
EBR_0_COUNTER_0 : boolean;
ASSIGN
init(EBR_0_PAST_1) := FALSE;
next(EBR_0_PAST_1) := !a18;
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a46;
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a60;
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a64;
DEFINE
--ands
a8 := EBR_0_PAST_1;
a10 := FALSE;
a12 := !a8 & !a10;
a14 := !u;
a16 := u & !a12;
a18 := !a14 & !a16;
a22 := !EBR_0_ERROR_0 & EBR_0_PAST_1;
a24 := !u & !a2;
a26 := !EBR_0_ERROR_0 & a24;
a28 := !EBR_0_ERROR_0 & !a2;
a30 := FALSE;
a32 := !a28;
a34 := !a30 & !a32;
a36 := FALSE;
a38 := !a26 & !a34;
a40 := !a36 & !a38;
a42 := FALSE;
a44 := !a22 & !a40;
a46 := !a42 & !a44;
a52 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a54 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a56 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a58 := !a54 & !a56;
a60 := !a52 & !a58;
a62 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a64 := !a62 & EBR_0_COUNTER_0;
a66 := EBR_0_ERROR_0;
a2 := TRUE;
--outputs
formula := !a66;
c := a2;

LTLSPEC  u U c

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  (u U (c & time<=maxtime))};
