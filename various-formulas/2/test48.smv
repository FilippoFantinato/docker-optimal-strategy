MODULE controller(p)
--latches
VAR
EBR_0_SPECF_1 : boolean;
EBR_0_ERROR_0 : boolean;
EBR_0_COUNTER_1 : boolean;
EBR_0_COUNTER_0 : boolean;
EBR_0_COUNTER_2 : boolean;
EBR_0_COUNTER_3 : boolean;
ASSIGN
init(EBR_0_SPECF_1) := FALSE;
next(EBR_0_SPECF_1) := !a2;
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a36;
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a50;
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a58;
init(EBR_0_COUNTER_2) := FALSE;
next(EBR_0_COUNTER_2) := !a74;
init(EBR_0_COUNTER_3) := FALSE;
next(EBR_0_COUNTER_3) := !a92;
DEFINE
--ands
a16 := EBR_0_COUNTER_1 & EBR_0_COUNTER_0;
a18 := !EBR_0_COUNTER_2 & !a16;
a20 := !EBR_0_COUNTER_3 & a18;
a24 := !EBR_0_ERROR_0 & EBR_0_SPECF_1;
a26 := FALSE;
a28 := !a24;
a30 := !a26 & !a28;
a32 := FALSE;
a34 := !a20 & !a30;
a36 := !a32 & !a34;
a38 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a40 := a38 & EBR_0_COUNTER_2;
a42 := a40 & EBR_0_COUNTER_3;
a44 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a46 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a48 := !a44 & !a46;
a50 := !a42 & !a48;
a52 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a54 := a52 & EBR_0_COUNTER_2;
a56 := a54 & EBR_0_COUNTER_3;
a58 := !a56 & EBR_0_COUNTER_0;
a60 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a62 := a60 & EBR_0_COUNTER_2;
a64 := a62 & EBR_0_COUNTER_3;
a66 := EBR_0_COUNTER_1 & EBR_0_COUNTER_0;
a68 := a66 & EBR_0_COUNTER_2;
a70 := !EBR_0_COUNTER_2 & !a66;
a72 := !a68 & !a70;
a74 := !a64 & !a72;
a76 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a78 := a76 & EBR_0_COUNTER_2;
a80 := a78 & EBR_0_COUNTER_3;
a82 := EBR_0_COUNTER_1 & EBR_0_COUNTER_0;
a84 := EBR_0_COUNTER_2 & a82;
a86 := a84 & EBR_0_COUNTER_3;
a88 := !EBR_0_COUNTER_3 & !a84;
a90 := !a86 & !a88;
a92 := !a80 & !a90;
a94 := EBR_0_ERROR_0;
a2 := TRUE;
--outputs
formula := !a94;
q := a2;

LTLSPEC  (X X X p) U (X X F q)

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  ((X X X p) U (X X F (q & time<=maxtime)))};
