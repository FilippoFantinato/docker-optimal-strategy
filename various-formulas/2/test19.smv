MODULE controller(u)
--latches
VAR
EBR_0_COUNTER_0 : boolean;
EBR_0_COUNTER_2 : boolean;
EBR_0_PAST_1 : boolean;
EBR_0_ERROR_0 : boolean;
EBR_0_COUNTER_1 : boolean;
ASSIGN
init(EBR_0_COUNTER_0) := FALSE;
next(EBR_0_COUNTER_0) := !a16;
init(EBR_0_COUNTER_2) := FALSE;
next(EBR_0_COUNTER_2) := !a30;
init(EBR_0_PAST_1) := FALSE;
next(EBR_0_PAST_1) := !a52;
init(EBR_0_ERROR_0) := FALSE;
next(EBR_0_ERROR_0) := !a88;
init(EBR_0_COUNTER_1) := FALSE;
next(EBR_0_COUNTER_1) := !a100;
DEFINE
--ands
a10 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a14 := a10 & EBR_0_COUNTER_2;
a16 := !a14 & EBR_0_COUNTER_0;
a18 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a20 := a18 & EBR_0_COUNTER_2;
a22 := EBR_0_COUNTER_1 & EBR_0_COUNTER_0;
a24 := a22 & EBR_0_COUNTER_2;
a26 := !EBR_0_COUNTER_2 & !a22;
a28 := !a24 & !a26;
a30 := !a20 & !a28;
a32 := !EBR_0_COUNTER_2 & !EBR_0_COUNTER_1;
a36 := EBR_0_PAST_1;
a38 := FALSE;
a40 := !a36 & !a38;
a42 := !u;
a44 := u & !a40;
a46 := !a42 & !a44;
a48 := FALSE;
a50 := !a32 & !a46;
a52 := !a48 & !a50;
a54 := !EBR_0_COUNTER_2 & !EBR_0_COUNTER_1;
a58 := !EBR_0_ERROR_0 & EBR_0_PAST_1;
a60 := !u & !a2;
a62 := !EBR_0_ERROR_0 & a60;
a64 := !EBR_0_ERROR_0 & !a2;
a66 := FALSE;
a68 := !a64;
a70 := !a66 & !a68;
a72 := FALSE;
a74 := !a62 & !a70;
a76 := !a72 & !a74;
a78 := FALSE;
a80 := !a58 & !a76;
a82 := !a78 & !a80;
a84 := FALSE;
a86 := !a54 & !a82;
a88 := !a84 & !a86;
a90 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a92 := a90 & EBR_0_COUNTER_2;
a94 := EBR_0_COUNTER_0 & EBR_0_COUNTER_1;
a96 := !EBR_0_COUNTER_1 & !EBR_0_COUNTER_0;
a98 := !a94 & !a96;
a100 := !a92 & !a98;
a102 := EBR_0_ERROR_0;
a2 := TRUE;
--outputs
formula := !a102;
c := a2;

LTLSPEC  X X (u U c)

VAR       time: 0..3;
FROZENVAR maxtime: 0..3;
ASSIGN
  init(time) := 0;
  next(time) := case time < 3 : time + 1; TRUE: time; esac;
  PARSYNTH r := { maxtime | VALID  (X X (u U (c & time<=maxtime)))};
